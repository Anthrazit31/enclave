{
  "name": "phoenix-industries-backend",
  "version": "1.0.0",
  "description": "Backend server for Phoenix Industries cyberpunk terminal system",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "db:init": "npx prisma migrate dev --name init",
    "db:seed": "node prisma/seed.js",
    "db:studio": "npx prisma studio",
    "test": "jest",
    "lint": "eslint server/ --ext .js",
    "build": "echo 'No build step required for Node.js'"
  },
  "keywords": ["cyberpunk", "terminal", "authentication", "express", "sqlite"],
  "author": "Phoenix Industries",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "socket.io": "^4.7.4",
    "cookie-parser": "^1.4.6",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}